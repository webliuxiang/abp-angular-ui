@inject IWebHostEnvironment _env;

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>
        52ABP-PRO系统 @ViewBag.Title
    </title>
    <link href="~/yoyosoft/css/bootstrap.min.css" rel="stylesheet" asp-append-version="true" />
    @if (IsSectionDefined("CSS"))
    {@RenderSection("CSS", required:
false)}
</head>
<body>

    <div class="container  ">
        @RenderBody()

        <footer class="text-center mt-3  ">

            <p>
                Copyright  &copy; 2018-   @DateTime.Now.Year 当前版本 @AppVersionHelper.Version 项目启动时间 @AppVersionHelper.ReleaseDate  <a href="https://www.52abp.com/" class="text-primary" target="_blank">52ABP.com出品- @_env.EnvironmentName </a>
            </p>
        </footer>
    </div>

    @if (IsSectionDefined("Scripts"))
    {@RenderSection("Scripts", required:
false)}
</body>
</html>